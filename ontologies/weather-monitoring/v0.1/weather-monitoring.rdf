    <!-- http://www.w3id.com/whow/onto/weather-monitoring/Wind -->

    <owl:NamedIndividual rdf:about="http://www.w3id.com/whow/onto/weather-monitoring/Wind">
        <rdf:type rdf:resource="http://www.w3id.com/whow/onto/weather-monitoring/WeatherFeatureOfInterest"/>
        <rdfs:isDefinedBy rdf:resource="http://www.w3id.com/whow/onto/weather-monitoring"/>
