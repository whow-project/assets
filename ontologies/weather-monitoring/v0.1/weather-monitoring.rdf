<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.w3id.com/whow/onto/weather-monitoring"
     xml:base="http://www.w3id.com/whow/onto/weather-monitoring"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.w3id.com/whow/onto/weather-monitoring">
        <owl:imports rdf:resource="https://w3id.org/italia/env/onto/inspire-mf/"/>
        <owl:imports rdf:resource="https://w3id.org/italia/env/onto/place/"/>
        <dc:description rdf:resource="https://raw.githubusercontent.com/whow-project/semantic-assets/main/ontologies/images/weather-monitoring.png"/>
        <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-01-30</dct:issued>
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-01-30</dct:modified>
        <rdfs:label xml:lang="it">Ontologia per il monitoraggio meteo</rdfs:label>
        <rdfs:label xml:lang="en">Weather Monitoring Ontology</rdfs:label>
    </owl:Ontology>
    


