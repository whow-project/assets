    <!-- https://w3id.org/whow/onto/health-monitoring/CauseOfDeath -->

    <owl:Class rdf:about="https://w3id.org/whow/onto/health-monitoring/CauseOfDeath">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/env/onto/top/Concept"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/env/onto/top/isClassifiedBy"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/whow/onto/health-monitoring/GroupCauseOfDeath"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">La classe di tutte quelle circostanze/situazioni/condizioni morbose che portano direttamente alla morte della persona.</rdfs:comment>
        <rdfs:comment xml:lang="en">The class of all those circumstances/situations/pathological conditions that lead directly to a person&apos;s death.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/health-monitoring"/>
        <rdfs:label xml:lang="it">Causa di Morte</rdfs:label>
        <rdfs:label xml:lang="en">Cause of Death</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>



    <!-- https://w3id.org/whow/onto/health-monitoring/ClinicalCohort -->

    <owl:Class rdf:about="https://w3id.org/whow/onto/health-monitoring/ClinicalCohort">
