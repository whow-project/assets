    <!-- https://w3id.org/whow/onto/water-monitoring/lowerBound -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/lowerBound">
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/Range"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">The lower bound in a range of values</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">lower bound</rdfs:label>
    </owl:DatatypeProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/lowerBoundIncluded -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/lowerBoundIncluded">
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/Range"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">The property represents the lower bound in an interval of values that is included in the interval itself</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">lower bound included</rdfs:label>
    </owl:DatatypeProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/upperBound -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/upperBound">
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/Range"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">The upper bound in a range of values</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">upper bound</rdfs:label>
    </owl:DatatypeProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/upperBoundIncluded -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/upperBoundIncluded">
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/Range"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">The property represents the upper bound in an interval of values that is included in the interval itself</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">upper bound included</rdfs:label>
    </owl:DatatypeProperty>



