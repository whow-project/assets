<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/whow/onto/water-monitoring/"
     xml:base="https://w3id.org/whow/onto/water-monitoring/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
    <owl:Ontology rdf:about="https://w3id.org/whow/onto/water-monitoring">
        <owl:versionIRI rdf:resource="https://w3id.org/whow/onto/water-monitoring/0.2"/>
        <owl:imports rdf:resource="https://dati.isprambiente.it/ontology/ispra-emf/"/>
        <owl:imports rdf:resource="https://dati.isprambiente.it/ontology/ispra-plc/"/>
        <owl:imports rdf:resource="https://w3id.org/whow/onto/hydrography"/>
        <dc:description rdf:resource="https://raw.githubusercontent.com/whow-project/semantic-assets/main/ontologies/images/pattern-solution-water-observation_v2.jpg"/>
        <dct:description xml:lang="it">L&apos;ontologia utilizzata nel progetto WHOW per rappresentare le osservazioni relative a diversi aspetti dell&apos;acqua: qualità/inquinamento ed elementi fisici.</dct:description>
        <dct:description xml:lang="en">The ontology used in the WHOW project to represent the observations related to different aspects of water: quality/pollution and physical elements.</dct:description>
        <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-07-20</dct:issued>
        <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-11-14</dct:modified>
        <dct:title xml:lang="it">Ontologia del Monitoraggio delle Acque</dct:title>
        <dct:title xml:lang="en">Water Monitoring Ontology</dct:title>
        <rdfs:comment xml:lang="it">L&apos;ontologia utilizzata nel progetto WHOW per rappresentare le osservazioni relative a diversi aspetti dell&apos;acqua: qualità/inquinamento ed elementi fisici.</rdfs:comment>
        <rdfs:comment xml:lang="en">The ontology used in the WHOW project to represent the observations related to different aspects of water: quality/pollution and physical elements.</rdfs:comment>
        <rdfs:label xml:lang="it">Ontologia del Monitoraggio delle Acque</rdfs:label>
        <rdfs:label xml:lang="en">Water Monitoring Ontology</rdfs:label>
        <owl:versionInfo xml:lang="en">version 0.2 - revision of the depth property; added Range class and related properties</owl:versionInfo>
    </owl:Ontology>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- http://purl.org/dc/terms/issued -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/issued"/>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- https://dati.isprambiente.it/ontology/ispra-emf/hasFeatureOfInterest -->

    <owl:ObjectProperty rdf:about="https://dati.isprambiente.it/ontology/ispra-emf/hasFeatureOfInterest"/>



    <!-- https://w3id.org/whow/onto/water-monitoring/hasBiologicalAgent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/hasBiologicalAgent">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/whow/onto/water-monitoring/hasWaterObservablePropertyObject"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/whow/onto/water-monitoring/WaterBiologicalQualityParameterObservation"/>
                    <rdf:Description rdf:about="https://w3id.org/whow/onto/water-monitoring/WaterMicrobiologicalParameterObservation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/whow/onto/water-monitoring/BiologicalAgent"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">has biological agent</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/hasChemicalSubstance -->

    <owl:ObjectProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/hasChemicalSubstance">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/whow/onto/water-monitoring/hasWaterObservablePropertyObject"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/whow/onto/water-monitoring/WaterChemicalParameterObservation"/>
                    <rdf:Description rdf:about="https://w3id.org/whow/onto/water-monitoring/WaterPhysicoChemicalParameterObservation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/whow/onto/water-monitoring/ChemicalSubstance"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">has chemical substance</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/hasDepth -->

    <owl:ObjectProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/hasDepth">
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterSample"/>
        <rdfs:range rdf:resource="https://dati.isprambiente.it/ontology/top/Value"/>
        <rdfs:comment xml:lang="en">Property used to represent the depth of a water body, typically of a lake.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">has depth</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/hasObservationSample -->

    <owl:ObjectProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/hasObservationSample">
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservation"/>
        <rdfs:range rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterSample"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">has observation sample</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/hasRadioactivityObject -->

    <owl:ObjectProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/hasRadioactivityObject">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/whow/onto/water-monitoring/hasWaterObservablePropertyObject"/>
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterRadioactivityObservation"/>
        <rdfs:range rdf:resource="https://w3id.org/whow/onto/water-monitoring/RadioactivityObject"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">has radioactivity object</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/hasResult -->

    <owl:ObjectProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/hasResult">
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservation"/>
        <rdfs:range rdf:resource="https://w3id.org/whow/onto/water-monitoring/ObservationValue"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">has result</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/hasWaterFeature -->

    <owl:ObjectProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/hasWaterFeature">
        <rdfs:subPropertyOf rdf:resource="https://dati.isprambiente.it/ontology/ispra-emf/hasFeatureOfInterest"/>
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservation"/>
        <rdfs:range rdf:resource="https://w3id.org/whow/onto/hydrography/WaterFeature"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">has water feature</rdfs:label>
        <owl:versionInfo xml:lang="en">we are planning to move it in hydrography ontology</owl:versionInfo>
    </owl:ObjectProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/hasWaterObservableProperty -->

    <owl:ObjectProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/hasWaterObservableProperty">
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservation"/>
        <rdfs:range rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservableProperty"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">has water observable property</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/hasWaterObservablePropertyObject -->

    <owl:ObjectProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/hasWaterObservablePropertyObject">
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservation"/>
        <rdfs:range rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservablePropertyObject"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:seeAlso xml:lang="en">has water observable property object</rdfs:seeAlso>
    </owl:ObjectProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/isClassifiedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/isClassifiedBy">
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservablePropertyObject"/>
        <rdfs:range rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservablePropertyObjectType"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">is classified by</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/isObtainedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/isObtainedBy">
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterSample"/>
        <rdfs:range rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterSampler"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">is obtained by</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/isSampleOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/isSampleOf">
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterSample"/>
        <rdfs:range rdf:resource="https://w3id.org/whow/onto/hydrography/WaterFeature"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">is sample of</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/whow/onto/water-monitoring/ofPropertyObject -->

    <owl:ObjectProperty rdf:about="https://w3id.org/whow/onto/water-monitoring/ofPropertyObject">
        <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservableProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservablePropertyObject"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">of property object</rdfs:label>
    </owl:ObjectProperty>



