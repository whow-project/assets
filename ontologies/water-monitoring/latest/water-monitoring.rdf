    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- http://www.w3.org/2004/02/skos/core#Concept -->

    <owl:Class rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>



    <!-- https://dati.isprambiente.it/ontology/ispra-emf/FeatureOfInterest -->

    <owl:Class rdf:about="https://dati.isprambiente.it/ontology/ispra-emf/FeatureOfInterest"/>



    <!-- https://dati.isprambiente.it/ontology/ispra-emf/MonitoringFacility -->

    <owl:Class rdf:about="https://dati.isprambiente.it/ontology/ispra-emf/MonitoringFacility"/>



    <!-- https://dati.isprambiente.it/ontology/ispra-emf/Observation -->

    <owl:Class rdf:about="https://dati.isprambiente.it/ontology/ispra-emf/Observation"/>



    <!-- https://dati.isprambiente.it/ontology/ispra-emf/ObservationValue -->

    <owl:Class rdf:about="https://dati.isprambiente.it/ontology/ispra-emf/ObservationValue"/>



    <!-- https://w3id.org/whow/onto/water-monitoring/BiologicalAgent -->

    <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring/BiologicalAgent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservablePropertyObject"/>
        <owl:disjointWith rdf:resource="https://w3id.org/whow/onto/water-monitoring/ChemicalSubstance"/>
        <owl:disjointWith rdf:resource="https://w3id.org/whow/onto/water-monitoring/RadioactivityObject"/>
        <rdfs:comment xml:lang="en">This class represents all the biological and microbiological agents that are available in the water be it surface and groundwater or drinking water (e.g., phytoplankton, macrophytes, phytobenthos, benthic invertebrate fauna and fish, bacteria such as coliforms).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">Biological Agent</rdfs:label>
    </owl:Class>



    <!-- https://w3id.org/whow/onto/water-monitoring/ChemicalSubstance -->

    <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring/ChemicalSubstance">
        <rdfs:subClassOf rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservablePropertyObject"/>
        <owl:disjointWith rdf:resource="https://w3id.org/whow/onto/water-monitoring/RadioactivityObject"/>
        <rdfs:comment xml:lang="en">This class represents the chemical substances. A controlled vocabulary can be constructed for this class based on the CAS numbering of the chemical substances.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">Chemical Substance</rdfs:label>
    </owl:Class>



    <!-- https://w3id.org/whow/onto/water-monitoring/DrinkingWaterObservation -->

    <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring/DrinkingWaterObservation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservation"/>
        <owl:disjointWith rdf:resource="https://w3id.org/whow/onto/water-monitoring/SurfaceOrGroundwaterObservation"/>
        <rdfs:comment xml:lang="en">The class of observations of water intended for human consumption.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">Drinking Water Observation</rdfs:label>
    </owl:Class>



    <!-- https://w3id.org/whow/onto/water-monitoring/ObservationValue -->

    <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring/ObservationValue">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://dati.isprambiente.it/ontology/top/Value"/>
                    <rdf:Description rdf:about="https://w3id.org/whow/onto/water-monitoring/Range"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">The result of an observation. It is a value  or a range of values which is charaterised by the value(s) ifself(themselves) and , when applicable, by a unit of measure.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">Observation Value</rdfs:label>
    </owl:Class>



    <!-- https://w3id.org/whow/onto/water-monitoring/RadioactivityObject -->

    <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring/RadioactivityObject">
        <rdfs:subClassOf rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservablePropertyObject"/>
        <rdfs:comment xml:lang="en">This class represents the radioactivity object. Examples are randon 22, nuclide, etc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">Radioactivity Object</rdfs:label>
    </owl:Class>



    <!-- https://w3id.org/whow/onto/water-monitoring/Range -->

    <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring/Range">
        <rdfs:subClassOf rdf:resource="https://dati.isprambiente.it/ontology/top/Characteristic"/>
        <rdfs:comment xml:lang="en">The class of value intervals. Examples: the value of the observation of the concentration of copepr is &lt;10, meaning between 0 and 10, etc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">Range (of Values)</rdfs:label>
    </owl:Class>



    <!-- https://w3id.org/whow/onto/water-monitoring/SurfaceOrGroundwaterObservation -->

    <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring/SurfaceOrGroundwaterObservation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterObservation"/>
        <rdfs:comment xml:lang="en">The class of observations of surface and ground waters.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">Surface or Groundwater Observation</rdfs:label>
    </owl:Class>



    <!-- https://w3id.org/whow/onto/water-monitoring/WaterBiologicalQualityParameterObservation -->

    <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring/WaterBiologicalQualityParameterObservation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/whow/onto/water-monitoring/SurfaceOrGroundwaterObservation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/whow/onto/water-monitoring/hasBiologicalAgent"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/whow/onto/water-monitoring/BiologicalAgent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/whow/onto/water-monitoring/hasBiologicalAgent"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/whow/onto/water-monitoring/BiologicalAgent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterHydromorphologicalParameterObservation"/>
        <owl:disjointWith rdf:resource="https://w3id.org/whow/onto/water-monitoring/WaterPhysicoChemicalParameterObservation"/>
        <rdfs:comment xml:lang="en">Observation of biological quality elements parameters according to the European Environmental Agency (EEA): phytoplankton, macrophytes, phytobenthos, benthic invertebrate fauna and fish. The most frequently classified biological  quality elements are, for rivers, benthic invertebrates, phytobenthos/other aquatic flora/macrophytes and fish; for lakes, phytoplankton; and for transitional and coastal waters, phytoplankton and benthic invertebrates. (2018)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/whow/onto/water-monitoring"/>
        <rdfs:label xml:lang="en">Water Biological Quality Parameter/Element Observation</rdfs:label>
    </owl:Class>



    <!-- https://w3id.org/whow/onto/water-monitoring/WaterChemicalParameterObservation -->

    <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring/WaterChemicalParameterObservation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/whow/onto/water-monitoring/DrinkingWaterObservation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/whow/onto/water-monitoring/hasChemicalSubstance"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/whow/onto/water-monitoring/ChemicalSubstance"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/whow/onto/water-monitoring/hasChemicalSubstance"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/whow/onto/water-monitoring/ChemicalSubstance"/>
            </owl:Restriction>
