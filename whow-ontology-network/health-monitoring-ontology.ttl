# baseURI: https://w3id.org/environmental-data/onto/hospitalization-ontology
# imports: http://purl.org/dc/terms/
# imports: http://www.w3.org/2006/vcard/ns
# imports: http://www.w3.org/ns/dcat
# imports: http://www.w3.org/ns/prov
# imports: http://xmlns.com/foaf/0.1/
# imports: https://dati.isprambiente.it/ontology/inspire-mf/
# imports: https://w3id.org/environmental-data/onto/water-monitoring-ontology
# imports: https://w3id.org/health-data/onto/CCStoICD9-data
# imports: https://w3id.org/italia/onto/Indicator

@prefix :        <https://w3id.org/environmental-data/onto/hospitalization-ontology/> .
@prefix CCStoICD9-data:  <https://w3id.org/health-data/onto/CCStoICD9-data/> .
@prefix ind:     <https://w3id.org/italia/onto/Indicator/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix w-mon:   <https://w3id.org/environmental-data/onto/water-monitoring-ontology/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/environmental-data/onto/hospitalization-ontology>
      rdf:type owl:Ontology ;
      owl:imports <http://purl.org/dc/terms/> , <http://www.w3.org/2006/vcard/ns> , <http://www.w3.org/ns/dcat> , <http://www.w3.org/ns/prov> , <http://xmlns.com/foaf/0.1/> , <https://dati.isprambiente.it/ontology/inspire-mf/> , <https://w3id.org/environmental-data/onto/water-monitoring-ontology> , <https://w3id.org/health-data/onto/CCStoICD9-data> , <https://w3id.org/italia/onto/Indicator> ;
      owl:versionInfo "Created with TopBraid Composer"^^xsd:string .

:ATS  rdf:type owl:Class ;
      rdfs:label "ATS "@en ;
      rdfs:subClassOf <https://dati.isprambiente.it/ontology/core/Entity> , :HealthConcept .

:ATScode
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :ATS ;
      rdfs:label "ATS code "@en ;
      rdfs:range xsd:string .

:DeathCause
      rdf:type owl:Class ;
      rdfs:label "Death Cause "@en ;
      rdfs:subClassOf :HealthConcept .

:HealthConcept
      rdf:type owl:Class ;
      rdfs:label "HealthConcept "@en ;
      rdfs:subClassOf <https://dati.isprambiente.it/ontology/core/Entity> ;
      skos:note "TODO: Temporaneo"^^xsd:string .

:HealthConditionsDeathCauseObservation
      rdf:type owl:Class ;
      rdfs:label "Health Condition Death Cause Observation "@en ;
      rdfs:subClassOf <https://dati.isprambiente.it/ontology/inspire-mf/Observation> , :HealthConcept .

:Hospitalization
      rdf:type owl:Class ;
      rdfs:label "Hospitalization "@en ;
      rdfs:subClassOf :HealthConcept .

:InfectiousDiseasesObservation
      rdf:type owl:Class ;
      rdfs:label "Infectious Diseases Observation "@en ;
      rdfs:subClassOf <https://dati.isprambiente.it/ontology/inspire-mf/Observation> , :HealthConcept .

:ageRangeOfPatient
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :InfectiousDiseasesObservation ;
      rdfs:label "age range of patient "@en ;
      rdfs:range xsd:string .

:existingVaccine
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :InfectiousDiseasesObservation ;
      rdfs:label "existing vaccine "@en ;
      rdfs:range xsd:boolean .

:genderOfPatient
      rdf:type owl:DatatypeProperty ;
      rdfs:domain
              [ rdf:type owl:Class ;
                owl:unionOf (:InfectiousDiseasesObservation :HealthConditionsDeathCauseObservation)
              ] ;
      rdfs:label "gender of patient "@en ;
      rdfs:range xsd:string .

:hasDeathCause
      rdf:type owl:ObjectProperty ;
      rdfs:domain :HealthConditionsDeathCauseObservation ;
      rdfs:label "has death cause "@en ;
      rdfs:range :DeathCause .

:hasHospitalizationRate
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Hospitalization ;
      rdfs:label "has hospitalization rate "@en ;
      rdfs:range xsd:float .

:hasNumberOfDeaths
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :HealthConditionsDeathCauseObservation ;
      rdfs:label "has number of deaths "@en ;
      rdfs:range xsd:float .

:numberOfMedicalPractices
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :InfectiousDiseasesObservation ;
      rdfs:label "number of medical practices "@en ;
      rdfs:range xsd:float .

:relatedToATS
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Hospitalization ;
      rdfs:label "related to ATS "@en ;
      rdfs:range :ATS .

:relatedToDiagnosis
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Hospitalization ;
      rdfs:label "related to diagnosis "@en ;
      rdfs:range <https://w3id.org/health-data/onto/CCS-ontology/CCSd> .
