        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>/WaterBiologicalQualityParameterObservation</IRI>
        <Literal xml:lang="en">Water Biological Quality Parameter/Element Observation</Literal>
    </AnnotationAssertion>
