  <rdf:Description rdf:about="http://www.w3.org/ns/sosa/observedProperty">
    <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring-ontology/WaterObservation"/>
    <rdfs:range rdf:resource="http://www.w3.org/ns/sosa/ObservableProperty"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/fundedBy">
    <rdfs:domain rdf:resource="https://w3id.org/whow/onto/water-monitoring-ontology/WaterPhysicoChemicalParameterObservation"/>
  </rdf:Description>

  <rdf:Description rdf:about="https://dati.isprambiente.it/ontology/core/UnitOfMeasure">
