  <owl:Ontology rdf:about="https://w3id.org/whow/onto/water-monitoring-ontology">
    <owl:imports rdf:resource="http://www.w3.org/ns/sosa/"/>
    <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
    <owl:imports rdf:resource="http://purl.org/dc/terms/"/>
    <owl:imports rdf:resource="https://w3id.org/whow/onto/hydrography-ontology"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <owl:imports rdf:resource="https://dati.isprambiente.it/ontology/inspire-mf-ssn-sosa-aligns/"/>
    <owl:imports rdf:resource="https://dati.isprambiente.it/ontology/places/"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Created with TopBraid Composer</owl:versionInfo>
    <skos:definition xml:lang="en">Ontology for describing WHOW project concepts for the water monitoring domain</skos:definition>
    <skos:note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TODO: valutare di rinominare in water-observation-ontology</skos:note>
    <skos:prefLabel xml:lang="en">Water Monitoring Ontology</skos:prefLabel>
  </owl:Ontology>

  <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring-ontology/BiologicalQualityElementsObservation">
    <rdfs:label xml:lang="en">Biological quality elements Parameter Observation</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/whow/onto/water-monitoring-ontology/SurfaceOrGroundwaterObservation"/>
    <skos:definition xml:lang="en">Observation of biological quality elements according to the European Environmental Agency (EEA): phytoplankton, macrophytes, phytobenthos, benthic invertebrate fauna and fish. The most frequently classified biological  quality elements are, for rivers, benthic invertebrates, phytobenthos/other aquatic flora/macrophytes and fish; for lakes, phytoplankton; and for transitional and coastal waters, phytoplankton and benthic invertebrates. (2018)</skos:definition>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring-ontology/ChemicalSubstance">
    <rdfs:label xml:lang="en">Chemical Agent</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/whow/onto/water-monitoring-ontology/WaterObservablePropertyObject"/>
    <owl:disjointWith rdf:resource="https://w3id.org/whow/onto/water-monitoring-ontology/RadioactivityObject"/>
    <owl:disjointWith rdf:resource="https://w3id.org/whow/onto/water-monitoring-ontology/MicroBiologicalAgent"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring-ontology/DrinkingWaterObservation">
    <rdfs:label xml:lang="en">Drinking Water Observation </rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/whow/onto/water-monitoring-ontology/WaterObservation"/>
    <owl:disjointWith rdf:resource="https://w3id.org/whow/onto/water-monitoring-ontology/SurfaceOrGroundwaterObservation"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring-ontology/MicroBiologicalAgent">
    <rdfs:label xml:lang="en">Microbiological agent </rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/whow/onto/water-monitoring-ontology/WaterObservablePropertyObject"/>
    <owl:disjointWith rdf:resource="https://w3id.org/whow/onto/water-monitoring-ontology/RadioactivityObject"/>
    <owl:disjointWith rdf:resource="https://w3id.org/whow/onto/water-monitoring-ontology/ChemicalSubstance"/>
  </owl:Class>

  <owl:Class rdf:about="https://w3id.org/whow/onto/water-monitoring-ontology/ObservationValue">
