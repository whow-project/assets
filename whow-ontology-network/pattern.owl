    <rdfs:label xml:lang="en">WHOW Concept </rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <skos:prefLabel xml:lang="en">WHOW Concept </skos:prefLabel>
  </owl:Class>

