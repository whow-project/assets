hydro:WaterBody
      rdfs:subClassOf <https://w3id.org/environmental-data/onto/water-monitoring-ontology/WaterFeature> .

<https://w3id.org/environmental-data/onto/water-monitoring-ontology/BiologicalQualityElementsObservation>
      rdf:type owl:Class ;
      rdfs:label "Biological quality elements Parameter Observation"@en ;
      rdfs:subClassOf <https://w3id.org/environmental-data/onto/water-monitoring-ontology/SurfaceOrGroundwaterObservation> ;
      skos:definition "Observation of biological quality elements according to the European Environmental Agency (EEA): phytoplankton, macrophytes, phytobenthos, benthic invertebrate fauna and fish. The most frequently classified biological  quality elements are, for rivers, benthic invertebrates, phytobenthos/other aquatic flora/macrophytes and fish; for lakes, phytoplankton; and for transitional and coastal waters, phytoplankton and benthic invertebrates. (2018)"@en .

<https://w3id.org/environmental-data/onto/water-monitoring-ontology/ChemicalSubstance>
      rdf:type owl:Class ;
      rdfs:label "Chemical Agent"@en ;
      rdfs:subClassOf <https://w3id.org/environmental-data/onto/water-monitoring-ontology/WaterObservablePropertyObject> .

<https://w3id.org/environmental-data/onto/water-monitoring-ontology/DrinkingWaterObservation>
      rdf:type owl:Class ;
      rdfs:label "Drinking Water Observation "@en ;
      rdfs:subClassOf <https://w3id.org/environmental-data/onto/water-monitoring-ontology/WaterObservation> .

<https://w3id.org/environmental-data/onto/water-monitoring-ontology/MicroBiologicalAgent>
      rdf:type owl:Class ;
      rdfs:label "Microbiological agent "@en ;
