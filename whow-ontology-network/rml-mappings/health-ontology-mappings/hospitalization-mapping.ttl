##########################################################
## LOGICAL SOURCES TO BE USED
##########################################################

# #LogicalSourceSostanzeChimiche
<#LogicalSourceHospitalization> a rml:BaseSource ;
    rml:source [
      a csvw:Table;
      #csvw:url "Monitoraggio_laghi_dati_puliti.csv";
      csvw:url "Dataset-Assistenza-Ospedaliera-per-ACC-e-ATS-xlsx.csv";

      csvw:dialect [
        a csvw:Dialect;
        csvw:delimiter ","
      ]
    ];
    rml:referenceFormulation ql:CSV .

############# CSMapping ###########


<#HospitalizationMapping> a rr:TriplesMap ;
rml:logicalSource <#LogicalSourceHospitalization> ;
rr:subjectMap [
  rr:template "hospitalization/{ANNO}-{CODICE ATS RESIDENZA}-{COORDINATA GEOGRAFICA X}-{COORDINATA GEOGRAFICA Y}-{CCS}-{TASSO RICOVERI STESSA ATS}";
  rr:termType rr:IRI;
  rr:class w-health:Hospitalization
] ;
rr:predicateObjectMap [
  rr:predicate rdfs:label;
  rr:objectMap [ rr:template "Hospitalization for {ANNO}, {CODICE ATS RESIDENZA} in ATS {CODICE ATS RESIDENZA}." ; rr:language "en" ]
];
rr:predicateObjectMap [
  rr:predicate w-health:hasHospitalizationRate;
  rr:objectMap [ rr:template "{TASSO RICOVERI STESSA ATS}"; rr:datatype xsd:float ]
];
rr:predicateObjectMap [
  rr:predicate ispra-top:time;
  rr:objectMap [ rr:template "{ANNO}"; rr:datatype xsd:date ]
];
rr:predicateObjectMap [
  rr:predicate w-health:relatedToDiagnosis;
  rr:objectMap [ rr:template "https://w3id.org/environmental-data/data/health-monitoring-ontology/CCS-d/{CCS}"; rr:termType rr:IRI ] #da vedere uri
];
rr:predicateObjectMap [
  rr:predicate w-health:relatedToATS;
  rr:objectMap [rr:template "https://w3id.org/environmental-data/data/health-monitoring-ontology/ATS/{CODICE ATS RESIDENZA}"; rr:termType rr:IRI ]
  ];

rr:predicateObjectMap [
  rr:predicate ispra-top:hasLocation;
  rr:objectMap [ rr:template "{COORDINATA GEOGRAFICA X}-{COORDINATA GEOGRAFICA Y}"; rr:datatype xsd:string ] #da fare riconcil. luoghi? #URI
].


<#ATSMapping> a rr:TriplesMap ;
rml:logicalSource <#LogicalSourceHospitalization> ;
rr:subjectMap [
    rr:template "https://w3id.org/environmental-data/data/health-monitoring-ontology/ATS/{CODICE ATS RESIDENZA}";
    rr:termType rr:IRI;
    rr:class w-health:ATS
  ] .
