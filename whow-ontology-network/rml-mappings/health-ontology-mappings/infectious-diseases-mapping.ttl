## LOGICAL SOURCES TO BE USED
##########################################################

# #LogicalSourceSostanzeChimiche
<#LogicalSourceInfectiousDiseases> a rml:BaseSource ;
    rml:source [
      a csvw:Table;
      #csvw:url "Monitoraggio_laghi_dati_puliti.csv";
      csvw:url "Malattie_infettive_Tassi_Regione_Lombardia_per_sesso_e_et_.csv";

      csvw:dialect [
        a csvw:Dialect;
        csvw:delimiter ","
      ]
    ];
    rml:referenceFormulation ql:CSV .

############# CSMapping ###########


<#InfectiousDiseasesMapping> a rr:TriplesMap ;
rml:logicalSource <#LogicalSourceInfectiousDiseases> ;
rr:subjectMap [
  rr:template "infectious-diseases-observation/{Patologia}-{Inizio Sintomi Anno}-{Sesso}-{Età Classe}-{Numero Pratiche}-{Popolazione Residente}";
  rr:termType rr:IRI;
  rr:class w-health:InfectiousDiseasesObservation
] ;
rr:predicateObjectMap [
  rr:predicate rdfs:label;
  rr:objectMap [ rr:template "Infectious disease observation for disease {Patologia} in {Inizio Sintomi Anno}, in Lombardy." ; rr:language "en" ]
];
rr:predicateObjectMap [
  rr:predicate ispra-top:hasLocation;
  rr:objectMap [ rr:template "Lombardy" ] #attenzione: deve essere uguale agli altri + come mappi ITALIA...
];
rr:predicateObjectMap [
  rr:predicate w-health:relatedToDiagnosis;
  rr:objectMap <#CCSdMapping>
];
rr:predicateObjectMap [
  rr:predicate w-health:genderOfPatient;
  rr:objectMap [ rr:template "{GENERE}"; rr:datatype xsd:string ]
];
rr:predicateObjectMap [
  rr:predicate w-health:existingVaccine;
  rr:objectMap [ rr:template "{VACCINO}"; rr:datatype xsd:boolean ]
];
rr:predicateObjectMap [
  rr:predicate w-health:ageRangeOfPatient;
  rr:objectMap [ rr:template "{Età Classe}"; rr:datatype xsd:string ]
];
rr:predicateObjectMap [
  rr:predicate w-health:numberOfMedicalPractices;
  rr:objectMap [ rr:template "{Numero Pratiche}"; rr:datatype xsd:float ]
];
rr:predicateObjectMap [
  rr:predicate ispra-top:time;
  rr:objectMap [ rr:template "{Inizio Sintomi Anno}"; rr:datatype xsd:date ]
].



<#CCSdMapping> a rr:TriplesMap ;
rml:logicalSource <#LogicalSourceHospitalization> ;
rr:subjectMap [
  rr:template "CCS-d/{CCS}";#riguardare
  rr:termType rr:IRI;
  rr:class w-health:CCSd
] .
