##########################################################
## LOGICAL SOURCES TO BE USED
##########################################################

# #LogicalSourceSostanzeChimiche
<#LogicalSourceHealthCondition> a rml:BaseSource ;
    rml:source [
      a csvw:Table;
      #csvw:url "Monitoraggio_laghi_dati_puliti.csv";
      csvw:url "Dataset-condizioni-di-salute-per-provincia-genere-e-causa-csv.csv";

      csvw:dialect [
        a csvw:Dialect;
        csvw:delimiter ","
      ]
    ];
    rml:referenceFormulation ql:CSV .

############# CSMapping ###########


<#HealthConditionMapping> a rr:TriplesMap ;
rml:logicalSource <#LogicalSourceHealthCondition> ;
rr:subjectMap [
  rr:template "health-condition-observation/{ANNO}-{PROVINCIA}-{GENERE}-{CAUSA DI MORTE}-{COORDINATA X}-{COORDINATA Y}-{NUMERO DECESSI POPOLAZIONE}";
  rr:termType rr:IRI;
  rr:class w-health:HealthConditionDeathCauseObservation
] ;
rr:predicateObjectMap [
  rr:predicate rdfs:label;
  rr:objectMap [ rr:template "Health condition observation for {ANNO}, {PROVINCIA}" ; rr:language "en" ]
];
rr:predicateObjectMap [
  rr:predicate w-health:hasDeathCause;
  rr:objectMap [ rr:template "https://w3id.org/environmental-data/data/death-cause/{CAUSA DI MORTE}" ]
];
rr:predicateObjectMap [
  rr:predicate ispra-top:hasLocation;
  rr:objectMap [ rr:template "https://w3id.org/environmental-data/data/province/{PROVINCIA}" ] #attenzione: deve essere uguale agli altri + come mappi ITALIA...
];
rr:predicateObjectMap [
  rr:predicate w-health:genderOfPatient;
  rr:objectMap [ rr:template "https://w3id.org/environmental-data/data/gender/{GENERE}"; rr:datatype xsd:string ]
];
rr:predicateObjectMap [
  rr:predicate ispra-top:time;
  rr:objectMap [ rr:template "https://w3id.org/environmental-data/data/year/{ANNO}"; rr:datatype xsd:date ]
].



<#DeathCauseMapping> a rr:TriplesMap ;
rml:logicalSource <#LogicalSourceHealthCondition> ;
rr:subjectMap [
  rr:template "https://w3id.org/environmental-data/data/death-cause/{CAUSA DI MORTE}";
  rr:termType rr:IRI;
  rr:class w-health:DeathCause
] .
