# baseURI: https://whowproject.eu/ontologies/water-monitoring-ontology
# imports: http://purl.org/dc/terms/
# imports: http://www.w3.org/2004/02/skos/core
# imports: http://www.w3.org/ns/ssn/
# imports: http://xmlns.com/foaf/0.1/
# imports: https://dati.isprambiente.it/ontology/inspire-mf-ssn-sosa-aligns/
# imports: https://dati.isprambiente.it/ontology/places/

@prefix :        <https://whowproject.eu/ontologies/water-monitoring-ontology/> .
@prefix inspire-mf:  <https://dati.isprambiente.it/ontology/inspire-mf/> .
@prefix inspire-mf-ssn-sosa-aligns:  <https://dati.isprambiente.it/ontology/inspire-mf-ssn-sosa-aligns/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix places:  <https://dati.isprambiente.it/ontology/places/> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sdo:     <http://schema.org/> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

<http://www.w3.org/ns/sosa/observedProperty>
      rdfs:domain :WaterObservation ;
      rdfs:range <http://www.w3.org/ns/sosa/ObservableProperty> ;
      sdo:domainIncludes :WaterObservation .

<https://dati.isprambiente.it/ontology/core/UnitOfMeasure>
      skos:note "TODO: trovare UM esterne a cui puntare"^^xsd:string .

<https://whowproject.eu/ontologies/water-monitoring-ontology>
      rdf:type owl:Ontology ;
      owl:imports <http://purl.org/dc/terms/> , <http://www.w3.org/2004/02/skos/core> , <http://www.w3.org/ns/ssn/> , <http://xmlns.com/foaf/0.1/> , inspire-mf-ssn-sosa-aligns: , places: ;
      owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
      skos:definition "Ontology for describing WHOW project concepts for the water monitoring domain"@en ;
      skos:prefLabel "Water Monitoring Ontology"@en .

:ChemicalAgent
      rdf:type owl:Class ;
      rdfs:label "Chemical Agent"@en ;
      rdfs:subClassOf :ObservablePropertyAgent ;
      skos:prefLabel "Chemical Agent"@en .

:CoastalWaterBody
      rdf:type owl:Class ;
      rdfs:label "Coastal Water Body "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:prefLabel "Coastal Water Body "@en .

:GroundwaterBody
      rdf:type owl:Class ;
      rdfs:label "Groundwater Body "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:prefLabel "Groundwater Body "@en .

:LTLECOObservation
      rdf:type owl:Class ;
      rdfs:label "LTLECO Observation "@en ;
      rdfs:subClassOf :WaterQualityParameterObservation ;
      skos:prefLabel "LTLECO Observation "@en .

:LakeWaterBody
      rdf:type owl:Class ;
      rdfs:label "Lake Water Body "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:note "TODO: disaccoppiare il concetto di CorpoIdrico da quello di Lago a livello geografico? Class \"Lake\""^^xsd:string ;
      skos:prefLabel "Lake Water Body "@en .

:MarineWaters
      rdf:type owl:Class ;
      rdfs:label "Marine Waters "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:prefLabel "Marine Waters "@en .

:Metadata
      rdf:type owl:Class ;
      rdfs:label "Metadata "@en ;
      rdfs:subClassOf :WHOWConcept ;
      skos:prefLabel "Metadata "@en .

:NuclideAgent
      rdf:type owl:Class ;
      rdfs:label "Nuclide Agent"@en ;
      rdfs:subClassOf :ObservablePropertyAgent ;
      skos:note "specializzare la property da Observation a ObservationPropertyAgent o tenere sempre un generico \"relatedToPropertyAgent\"...(diversamente da come modellato adesso hasNuclide, o hasChemicalAgentType? + possiamo chiamarlo RadioactivityAgent?"^^xsd:string ;
      skos:prefLabel "Nuclide Agent"@en .

:ObservablePropertyAgent
      rdf:type owl:Class ;
      rdfs:label "Observable Property Agent "@en ;
      rdfs:subClassOf skos:Concept , <https://dati.isprambiente.it/ontology/core/Agent> , :WHOWConcept ;
      skos:prefLabel "Observable Property Agent "@en .

:RiverWaterBody
      rdf:type owl:Class ;
      rdfs:label "River Water Body "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:prefLabel "River Water Body "@en .

:SpatialSamplingFeature
      rdf:type owl:Class ;
      rdfs:label "Spatial Sampling Feature "@en ;
      rdfs:subClassOf <https://dati.isprambiente.it/ontology/core/Entity> , :WHOWConcept ;
      skos:note "TODO: partOf tra spatial sampling feature e water feature o creare una nostra proprietà?"^^xsd:string ;
      skos:prefLabel "Spatial Sampling Feature "@en .

:TerritorialWaters
      rdf:type owl:Class ;
      rdfs:label "Territorial Waters "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:prefLabel "Territorial Waters "@en .

:TransitionalWaterBody
      rdf:type owl:Class ;
      rdfs:label "Transitional Water Body "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:prefLabel "Transitional Water Body "@en .

:WHOWConcept
      rdf:type owl:Class ;
      rdfs:comment "An abstract class collecting all the WHOW concepts."@en ;
      rdfs:label "WHOW Concept "@en ;
      rdfs:subClassOf owl:Thing ;
      skos:prefLabel "WHOW Concept "@en .

:WaterBasin
      rdf:type owl:Class ;
      rdfs:label "Water Basin "@en ;
      rdfs:subClassOf :WaterFeature ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :relatedWaterFeature
              ] ;
      skos:note "TODO: Per considerare i sublacuali e i postlacuali si potrebbe introdurre una relazione ricorsiva tra waterbody e waterbody"^^xsd:string ;
      skos:prefLabel "Water Basin "@en .

:WaterBioindicatorObservation
      rdf:type owl:Class ;
      rdfs:label "Water Bioindicator Observation"@en ;
      rdfs:subClassOf :WaterObservation ;
      skos:prefLabel "Water Bioindicator Observation"@en .

:WaterBioticIndexObservation
      rdf:type owl:Class ;
      rdfs:label "Water Biotic Index Observation"@en ;
      rdfs:subClassOf :WaterObservation ;
      skos:prefLabel "Water Biotic Index Observation"@en .

:WaterBody
      rdf:type owl:Class ;
      rdfs:label "Water Body "@en ;
      rdfs:subClassOf :WaterFeature ;
      skos:prefLabel "Water Body "@en .

:WaterChemicalAgentObservation
      rdf:type owl:Class ;
      rdfs:label "Water Chemical Agent Observation"@en ;
      rdfs:subClassOf :WaterObservation ;
      skos:prefLabel "Water Chemical Agent Observation"@en .

:WaterFeature
      rdf:type owl:Class ;
      rdfs:label "Water feature "@en ;
      rdfs:subClassOf inspire-mf:FeatureOfInterest , :WHOWConcept ;
      skos:prefLabel "Water feature "@en .

:WaterFeatureType
      rdf:type owl:Class ;
      rdfs:label "Water feature type "@en ;
      rdfs:subClassOf skos:Concept , :WHOWConcept ;
      skos:prefLabel "Water feature type "@en .

:WaterMonitoringStation
      rdf:type owl:Class ;
      rdfs:label "Water Monitoring Station"@en ;
      rdfs:subClassOf inspire-mf:Platform , :WHOWConcept ;
      skos:prefLabel "Water Monitoring Station"@en .

:WaterObservation
      rdf:type owl:Class ;
      rdfs:label "Water Observation "@en ;
      rdfs:subClassOf inspire-mf:Observation , :WHOWConcept ;
      skos:prefLabel "Water Observation "@en .

:WaterQualityParameterObservation
      rdf:type owl:Class ;
      rdfs:label "Water Quality Parameter Observation "@en ;
      rdfs:subClassOf :WaterObservation ;
      skos:prefLabel "Water Quality Parameter Observation "@en .

:WaterRadioactivityObservation
      rdf:type owl:Class ;
      rdfs:label "Water Radioactivity Observation"@en ;
      rdfs:subClassOf :WaterObservation ;
      skos:prefLabel "Water Radioactivity Observation"@en .

:WaterSamplingFeature
      rdf:type owl:Class ;
      rdfs:label "Water Sampling Feature "@en ;
      rdfs:subClassOf :SpatialSamplingFeature ;
      skos:prefLabel "Water Sampling Feature "@en .

:WaterSamplingType
      rdf:type owl:Class ;
      rdfs:label "Water Sampling Type "@en ;
      rdfs:subClassOf <https://dati.isprambiente.it/ontology/core/Entity> , :WHOWConcept ;
      skos:prefLabel "Water Sampling Type "@en .

:WaterSamplingValue
      rdf:type owl:Class ;
      rdfs:label "Water Sampling Value "@en ;
      rdfs:subClassOf <https://dati.isprambiente.it/ontology/core/Value> , :WHOWConcept ;
      skos:prefLabel "Water Sampling Value "@en .

:belongsToWaterBasin
      rdf:type owl:ObjectProperty ;
      rdfs:domain :SpatialSamplingFeature , :WaterBody ;
      rdfs:label "belongs to water basin "@en ;
      rdfs:range :WaterBasin .

:hasWaterFeatureType
      rdf:type owl:ObjectProperty ;
      rdfs:domain :WaterFeature ;
      rdfs:label "has water feature type "@en ;
      rdfs:range :WaterFeatureType ;
      skos:prefLabel "has water feature type "@en .

:hasWaterSamplingType
      rdf:type owl:ObjectProperty ;
      rdfs:domain :WaterSamplingFeature ;
      rdfs:label "has water sampling type "@en ;
      rdfs:range :WaterSamplingType ;
      skos:prefLabel "has water sampling type "@en .

:isSubWaterBasin
      rdf:type owl:ObjectProperty ;
      rdfs:domain :WaterBasin ;
      rdfs:label "is sub water basin "@en ;
      rdfs:range :WaterBasin ;
      skos:note "TODO: proprietà ricorsiva per indicare la parte pre- o sub- lacuale. Ha senso?"^^xsd:string .

:metadata
      rdf:type owl:ObjectProperty ;
      rdfs:domain :WaterObservation ;
      rdfs:label "metadata "@en ;
      skos:prefLabel "metadata "@en .

:relatedToPropertyAgent
      rdf:type rdf:Property ;
      rdfs:domain :WaterObservation ;
      rdfs:label "related to property agent"@en ;
      rdfs:range :ObservablePropertyAgent ;
      skos:prefLabel "related to property agent"@en .

:relatedWaterFeature
      rdf:type owl:ObjectProperty ;
      rdfs:domain
              [ rdf:type owl:Class ;
                owl:unionOf (:WaterObservation :WaterBasin)
              ] ;
      rdfs:label "related water feature"@en ;
      rdfs:range :WaterFeature ;
      rdfs:subPropertyOf inspire-mf:hasFeatureOfInterest ;
      skos:prefLabel "related water feature"@en .

:relatedWaterSamplingFeature
      rdf:type owl:ObjectProperty ;
      rdfs:domain :WaterObservation ;
      rdfs:label "related water sampling feature "@en ;
      skos:prefLabel "related water sampling feature "@en .

:takenFromStation
      rdf:type owl:ObjectProperty ;
      rdfs:domain inspire-mf:Observation ;
      rdfs:label "taken from station "@en ;
      rdfs:range :WaterMonitoringStation ;
      skos:prefLabel "taken from station "@en .
