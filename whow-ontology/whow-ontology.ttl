# baseURI: https://whowproject.eu/ontologies/whow-ontology
# imports: http://purl.org/dc/terms/
# imports: http://www.w3.org/2004/02/skos/core
# imports: http://www.w3.org/2006/vcard/ns
# imports: http://www.w3.org/ns/dcat
# imports: http://www.w3.org/ns/ssn/
# imports: http://xmlns.com/foaf/0.1/
# imports: https://dati.isprambiente.it/ontology/inspire-mf-ssn-sosa-aligns/
# imports: https://dati.isprambiente.it/ontology/inspire-mf/
# imports: https://dati.isprambiente.it/ontology/places/

@prefix :        <https://whowproject.eu/ontologies/whow-ontology#> .
@prefix inspire-mf:  <https://dati.isprambiente.it/ontology/inspire-mf/> .
@prefix inspire-mf-ssn-sosa-aligns:  <https://dati.isprambiente.it/ontology/inspire-mf-ssn-sosa-aligns/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix places:  <https://dati.isprambiente.it/ontology/places/> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

<https://dati.isprambiente.it/ontology/core/UnitOfMeasure>
      skos:note "TODO: trovare UM esterne a cui puntare"^^xsd:string .

<https://whowproject.eu/ontologies/whow-ontology>
      rdf:type owl:Ontology ;
      owl:imports <http://purl.org/dc/terms/> , <http://www.w3.org/2004/02/skos/core> , <http://www.w3.org/2006/vcard/ns> , <http://www.w3.org/ns/dcat> , <http://www.w3.org/ns/ssn/> , <http://xmlns.com/foaf/0.1/> , inspire-mf-ssn-sosa-aligns: , inspire-mf: , places: ;
      owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
      skos:definition "Ontology for describing WHOW project concepts about water monitoring"@en ;
      skos:prefLabel "WHOW Ontology"@en .

:BioindicatorObservation
      rdf:type owl:Class ;
      rdfs:label "Bioindicator Observation "@en ;
      rdfs:subClassOf :WaterObservation ;
      skos:prefLabel "Bioindicator Observation "@en .

:BioticIndexObservation
      rdf:type owl:Class ;
      rdfs:label "Biotic Index Observation "@en ;
      rdfs:subClassOf :WaterObservation ;
      skos:prefLabel "Biotic Index Observation "@en .

:ChemicalAgentConcentrationObservation
      rdf:type owl:Class ;
      rdfs:label "Chemical Agent Concentration Observation "@en ;
      rdfs:subClassOf :WaterObservation ;
      skos:prefLabel "Chemical Agent Concentration Observation "@en .

:CoastalWaterBody
      rdf:type owl:Class ;
      rdfs:label "Coastal Water Body "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:prefLabel "Coastal Water Body "@en .

:GroundwaterBody
      rdf:type owl:Class ;
      rdfs:label "Groundwater Body "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:prefLabel "Groundwater Body "@en .

:LTLECOObservation
      rdf:type owl:Class ;
      rdfs:label "LTLECO Observation "@en ;
      rdfs:subClassOf :WaterQualityParameterObservation ;
      skos:prefLabel "LTLECO Observation "@en .

:LakeChemicalAgentConcentrationObservation
      rdf:type owl:Class ;
      rdfs:label "Lake Chemical Agent Concentration Observation "@en ;
      rdfs:subClassOf :ChemicalAgentConcentrationObservation ;
      skos:prefLabel "Lake Chemical Agent Concentration Observation "@en .

:LakeWaterBody
      rdf:type owl:Class ;
      rdfs:label "Lake Water Body "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:prefLabel "Lake Water Body "@en .

:MarineWaters
      rdf:type owl:Class ;
      rdfs:label "Marine Waters "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:prefLabel "Marine Waters "@en .

:Metadata
      rdf:type owl:Class ;
      rdfs:label "Metadata "@en ;
      rdfs:subClassOf :WHOWConcept ;
      skos:prefLabel "Metadata "@en .

:MonitoringStation
      rdf:type owl:Class ;
      rdfs:label "Monitoring Station "@en ;
      rdfs:subClassOf inspire-mf:MonitoringFacility , :WHOWConcept ;
      skos:prefLabel "Monitoring Station "@en .

:RadioactivityConcentrationObservation
      rdf:type owl:Class ;
      rdfs:label "Radioactivity Concentration Observation "@en ;
      rdfs:subClassOf :ChemicalAgentConcentrationObservation ;
      skos:prefLabel "Radioactivity Concentration Observation "@en .

:RiverChemicalAgentConcentrationObservation
      rdf:type owl:Class ;
      rdfs:label "River Chemical Agent Concentration Observation "@en ;
      rdfs:subClassOf :ChemicalAgentConcentrationObservation ;
      skos:prefLabel "River Chemical Agent Concentration Observation "@en .

:RiverWaterBody
      rdf:type owl:Class ;
      rdfs:label "River Water Body "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:prefLabel "River Water Body "@en .

:SpatialSamplingFeature
      rdf:type owl:Class ;
      rdfs:label "Spatial Sampling Feature "@en ;
      rdfs:subClassOf <https://dati.isprambiente.it/ontology/core/Entity> , :WHOWConcept ;
      skos:prefLabel "Spatial Sampling Feature "@en .

:TerritorialWaters
      rdf:type owl:Class ;
      rdfs:label "Territorial Waters "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:prefLabel "Territorial Waters "@en .

:TransitionalWaterBody
      rdf:type owl:Class ;
      rdfs:label "Transitional Water Body "@en ;
      rdfs:subClassOf :WaterBody ;
      skos:prefLabel "Transitional Water Body "@en .

:WHOWConcept
      rdf:type owl:Class ;
      rdfs:comment "An abstract class collecting all the WHOW concepts."@en ;
      rdfs:label "WHOW Concept "@en ;
      rdfs:subClassOf owl:Thing ;
      skos:prefLabel "WHOW Concept "@en .

:WaterBasin
      rdf:type owl:Class ;
      rdfs:label "Water Basin "@en ;
      rdfs:subClassOf inspire-mf:FeatureOfInterest , :WHOWConcept ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:minCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :relatedWaterBody
              ] ;
      skos:note "TODO: Per considerare i sublacuali e i postlacuali si potrebbe introdurre una relazione ricorsiva tra waterbody e waterbody"^^xsd:string ;
      skos:prefLabel "Water Basin "@en .

:WaterBody
      rdf:type owl:Class ;
      rdfs:label "Water Body "@en ;
      rdfs:subClassOf inspire-mf:FeatureOfInterest , :WHOWConcept ;
      skos:prefLabel "Water Body "@en .

:WaterChemicalAgentType
      rdf:type owl:Class ;
      rdfs:label "Water Chemical Agent Type "@en ;
      rdfs:subClassOf <https://dati.isprambiente.it/ontology/core/Entity> , :WHOWConcept ;
      skos:prefLabel "Water Chemical Agent Type "@en .

:WaterObservation
      rdf:type owl:Class ;
      rdfs:label "Water Observation "@en ;
      rdfs:subClassOf inspire-mf:Observation , :WHOWConcept ;
      skos:prefLabel "Water Observation "@en .

:WaterObservationParameter
      rdf:type owl:Class ;
      rdfs:label "Water Observation Parameter"@en ;
      rdfs:subClassOf inspire-mf:ObservationParameter , :WHOWConcept ;
      skos:prefLabel "Water Observation Parameter"@en .

:WaterQualityParameterObservation
      rdf:type owl:Class ;
      rdfs:label "Water Quality Parameter Observation "@en ;
      rdfs:subClassOf :WaterObservation ;
      skos:prefLabel "Water Quality Parameter Observation "@en .

:WaterSamplingFeature
      rdf:type owl:Class ;
      rdfs:label "Water Sampling Feature "@en ;
      rdfs:subClassOf :SpatialSamplingFeature ;
      skos:prefLabel "Water Sampling Feature "@en .

:WaterSamplingValue
      rdf:type owl:Class ;
      rdfs:label "Water Sampling Value "@en ;
      rdfs:subClassOf <https://dati.isprambiente.it/ontology/core/Value> , :WHOWConcept ;
      skos:prefLabel "Water Sampling Value "@en .

:hasRelatedWaterBasin
      rdf:type owl:ObjectProperty ;
      rdfs:domain inspire-mf:Observation , :WaterSamplingFeature ;
      rdfs:label "has related water basin "@en ;
      rdfs:range :WaterBody ;
      skos:note "TODO: Che relazione Ã¨ questa? (Matteo)"^^xsd:string ;
      skos:prefLabel "has related water basin "@en .

:hasWaterChemicalAgentType
      rdf:type rdf:Property ;
      rdfs:domain :WaterObservation ;
      rdfs:label "has water chemical agent type "@en ;
      rdfs:range :WaterChemicalAgentType ;
      skos:prefLabel "has water chemical agent type "@en .

:hasWaterObservationParameter
      rdf:type owl:ObjectProperty ;
      rdfs:domain :WaterObservation ;
      rdfs:label "has water observation parameter"@en ;
      rdfs:range :WaterObservationParameter ;
      skos:prefLabel "has water observation parameter"@en .

:hasWaterSamplingType
      rdf:type owl:ObjectProperty ;
      rdfs:domain :WaterSamplingFeature ;
      rdfs:label "has water sampling type "@en ;
      rdfs:range :waterSamplingType ;
      skos:prefLabel "has water sampling type "@en .

:metadata
      rdf:type owl:ObjectProperty ;
      rdfs:domain :WaterObservation ;
      rdfs:label "metadata "@en ;
      skos:prefLabel "metadata "@en .

:relatedLake
      rdf:type owl:ObjectProperty ;
      rdfs:domain :LakeChemicalAgentConcentrationObservation ;
      rdfs:label "related lake "@en ;
      skos:prefLabel "related lake "@en .

:relatedWaterBody
      rdf:type owl:ObjectProperty ;
      rdfs:domain
              [ rdf:type owl:Class ;
                owl:unionOf (:WaterObservation :WaterBasin)
              ] ;
      rdfs:label "related water body "@en ;
      rdfs:range :WaterBody ;
      skos:prefLabel "related water body "@en .

:relatedWaterSamplingFeature
      rdf:type owl:ObjectProperty ;
      rdfs:domain :WaterObservation ;
      rdfs:label "related water sampling feature "@en ;
      skos:prefLabel "related water sampling feature "@en .

:takenFromStation
      rdf:type owl:ObjectProperty ;
      rdfs:domain :WaterObservation ;
      rdfs:label "taken from station "@en ;
      rdfs:range :MonitoringStation ;
      skos:prefLabel "taken from station "@en .

:waterSamplingType
      rdf:type owl:Class ;
      rdfs:label "Water Sampling Type "@en ;
      rdfs:subClassOf <https://dati.isprambiente.it/ontology/core/Entity> , :WHOWConcept ;
      skos:prefLabel "Water Sampling Type "@en .
